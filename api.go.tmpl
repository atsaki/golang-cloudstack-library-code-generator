
// Parameter of {{ title .Name }}
type {{ title .Name }}Parameter struct {
  {{ range $param := .Params }}
    {{ comment .Description }}
    {{ if isId $param.Name }}
      {{ camel $param.Name }} ID
    {{ else }}
      {{ camel $param.Name }} {{ paramType $param.Type }}
    {{ end }}
  {{ end }}
}

{{ comment .Description }}
{{ if isListAPI .Name }}
  func (c *Client) {{ title .Name }}(p *{{ title .Name }}Parameter) (objs []{{ objectName .Name | camel }}, err error) {
    err = c.request(p, &objs)
    return objs, err
  }
{{ else }}
  func (c *Client) {{ title .Name }}(p *{{ title .Name }}Parameter) (obj *{{ objectName .Name | camel }}, err error) {
    err = c.request(p, &obj)
    return obj, err
  }
{{ end }}
